<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
   					 	http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
   	<component>
		<component-type>Tree</component-type>
		<component-class>com.tags.CategoryTag</component-class>
	</component>
	<managed-bean>
		<managed-bean-name>login</managed-bean-name>
		<managed-bean-class>com.mysql.login</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>user</managed-bean-name>
		<managed-bean-class>com.user.user</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>uploadProducts</managed-bean-name>
		<managed-bean-class>com.products.UploadProducts</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>categoryHandler</managed-bean-name>
		<managed-bean-class>com.tags.CategoryHandler</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
</faces-config>
